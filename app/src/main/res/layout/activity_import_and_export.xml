<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Import_and_export">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Import and Export"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.161"
        app:layout_constraintStart_toEndOf="@+id/ie_backBtn"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="350dp"
        android:layout_height="100dp"
        android:layout_marginTop="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/box" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="350dp"
        android:layout_height="100dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:srcCompat="@drawable/box" />

    <ImageView
        android:id="@+id/import_btn"
        android:layout_width="175dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.269"
        app:srcCompat="@drawable/grey_box" />

    <TextView
        android:id="@+id/import_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="import"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/import_btn"
        app:layout_constraintEnd_toEndOf="@+id/import_btn"
        app:layout_constraintHorizontal_bias="0.59"
        app:layout_constraintStart_toStartOf="@+id/import_btn"
        app:layout_constraintTop_toTopOf="@+id/import_btn"
        app:layout_constraintVertical_bias="0.538" />

    <ImageView
        android:id="@+id/btn_plus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/import_btn"
        app:layout_constraintEnd_toEndOf="@+id/import_btn"
        app:layout_constraintHorizontal_bias="0.099"
        app:layout_constraintStart_toStartOf="@+id/import_btn"
        app:layout_constraintTop_toTopOf="@+id/import_btn"
        app:layout_constraintVertical_bias="0.538"
        app:srcCompat="@drawable/baseline_add_24" />

    <TextView
        android:id="@+id/import_btm_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Import contact from the file storage"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.855" />

    <ImageView
        android:id="@+id/export_btn"
        android:layout_width="175dp"
        android:layout_height="50dp"
        android:layout_marginStart="100dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.132"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.269"
        app:srcCompat="@drawable/grey_box" />

    <TextView
        android:id="@+id/export_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Export"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/export_btn"
        app:layout_constraintEnd_toEndOf="@+id/export_btn"
        app:layout_constraintHorizontal_bias="0.585"
        app:layout_constraintStart_toStartOf="@+id/export_btn"
        app:layout_constraintTop_toTopOf="@+id/export_btn" />

    <ImageView
        android:id="@+id/export_img"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/export_btn"
        app:layout_constraintEnd_toEndOf="@+id/export_btn"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toStartOf="@+id/export_btn"
        app:layout_constraintTop_toTopOf="@+id/export_btn"
        app:layout_constraintVertical_bias="0.411"
        app:srcCompat="@drawable/file_export" />

    <TextView
        android:id="@+id/export_btm_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Export your contacts to internal storage"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="@+id/imageView6"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.855" />

    <ImageButton
        android:id="@+id/ie_backBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/arrow_back"
        tools:layout_editor_absoluteY="32dp" />

</androidx.constraintlayout.widget.ConstraintLayout>